{
    "name": "dataprocessors/amqp-async",
    "type": "library",
    "description": "Async AMQP 0.9.1 client for PHP designed to work with Icicle.",
    "keywords": ["rabbitmq", "message", "queue"],
    "homepage": "https://github.com/DataProcessors/amqp-async/",
    "authors": [
        {
            "name" : "Data Processors"
        }
    ],
    "require": {
        "php": ">=7.0",
        "ext-bcmath": "*",
        "icicleio/icicle": "^0.9",
        "icicleio/socket": "^0.5",
        "icicleio/stream": "^0.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "DataProcessors\\AMQP\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DataProcessors\\AMQP\\tests\\Functional\\": "tests/Functional",
            "DataProcessors\\AMQP\\tests\\Unit\\": "tests/Unit"
        }
    },
    "license": "LGPL-2.1"
}
