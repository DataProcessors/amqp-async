{
    "name": "icicleio/amqp",
    "type": "library",
    "description": "Asynchronous AMQP 0.9.1 client for Icicle.",
    "keywords": ["rabbitmq", "message", "queue"],
    "homepage": "http://icicle.io",
    "authors": [
        {
            "name" : "Barry Brannan",
            "email" : "barry@dataprocessors.com.au"
        }
    ],
    "require": {
        "php": ">=7.0",
        "ext-bcmath": "*",
        "icicleio/icicle": "dev-master",
        "icicleio/socket": "dev-master",
        "icicleio/stream": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.6"
    },
    "autoload": {
        "psr-4": {
            "Icicle\\AMQP\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Icicle\\Tests\\AMQP\\Functional\\": "tests/Functional",
            "Icicle\\Tests\\AMQP\\Unit\\": "tests/Unit"
        }
    },
    "license": "LGPL-2.1"
}
